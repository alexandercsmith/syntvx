<div id="key-<%= key.id %>" class="admin-item">
  <div class="admin-item-info xl-col-3-4 lg-col-3-4 md-col-2-3 sm-col-1-2 xs-col-1-1">
    <span><b><%= key.name %></b> | <%= key.token %></span>
  </div>

  <div class="admin-item-links single xl-col-1-4 lg-col-1-4 md-col-1-3 sm-col-1-2 xs-col-1-1">
    <%= link_to admins_destroy_api_key_path(key), class: "delete", method: :delete, data: { confirm: "Are you sure?" } do %>
      <%= fa_icon "times" %>
    <% end %>
  </div>
</div>
