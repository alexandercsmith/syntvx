<div id="admin-<%= controller_name %>-<%= action_name %>" class="admin-page">
  <div id="admin-head">
    <div class="admin-head-panel">
      <h1><%= action_name.upcase %></h1>
    </div>
  </div>

  <h2>Categories</h2>

  <%= render partial: 'admins/categories/category', collection: @categories %>

  <h2>Languages</h2>

  <%= render partial: 'admins/languages/language', collection: @languages %>

  <h2>Tags</h2>

  <%= render partial: 'admins/tags/tag', collection: @tags %>

</div>
