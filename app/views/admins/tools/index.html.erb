<div id="admin-<%= controller_name %>-<%= action_name %>" class="admin-page">
  <div id="admin-head">
    <div class="admin-head-panel">
      <h1><%= controller_name.upcase %></h1>
      <%= link_to 'NEW', new_tool_path %>
    </div>
  </div>

  <%= render partial: 'admins/tools/search' %>

  <div id="admin-results">
    <div id="admin-results-panel">
      <%= render @tools %>
      <%= will_paginate @tools %>
    </div>
  </div>
</div>
